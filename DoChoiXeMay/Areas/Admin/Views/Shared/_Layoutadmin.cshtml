<body>
    <!-- Start Header Top Area -->
    <div class="header-top-area">
        <div style="width:95%">
            <div class="row" style="margin-right:0 !important">
                <div class="col-lg-3 col-md-3 col-sm-4 col-xs-8 col-8" style="text-align:center">
                    <div class="logo-Tek">
                        <p>
                            <img src="~/Areas/Admin/Content/img/logo/Teklogoo.png" alt="" class="avatartek" />
                            @if (Session["UserName"] != null && Session["avatar"] != null)
                            {
                                if (Session["avatar"].ToString().Trim() != "")
                                {
                                    var tt = Session["avatar"].ToString();
                                    <img src="~/Content/imgTeK/@tt" alt="" class="avatartek" />
                                }
                                else
                                {
                                    <img src="~/Content/imgTeK/Namn.png" alt="" style="width:34px" />
                                }
                                <span>@Session["UserName"].ToString().ToUpper()</span>
                            }
                        </p>

                    </div>
                </div>
                <div class="col-lg-5 col-md-5 col-sm-3 col-xs-4 col-4">
                    <div class="header-top-menu Topmn">
                        <ul class="nav navbar-nav notika-top-nav">
                            <li class="nav-item nc-al">
                                @if (CountMsg() > 0)
                                {
                                    <a title="Thông báo hệ thống." onclick="daxemMsg()" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle"><span><i class="notika-icon notika-alarm"></i></span><div class="spinner4 spinner-4"></div><div class="ntd-ctn"><span id="countsms">@CountMsg()</span></div></a>
                                }
                                else
                                {
                                    <a title="Thông báo hệ thống." data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle"><span><i class="notika-icon notika-alarm"></i></span><div class=""></div><div class="ntd-ctn"><span id="countsms">@CountMsg()</span></div></a>
                                }

                                <div role="menu" class="dropdown-menu message-dd notification-dd animated zoomIn">
                                    <div class="hd-mg-tt">
                                        <h2>Thông Báo Hệ Thống</h2>
                                    </div>
                                    <div class="hd-message-info" style="height:500px;overflow:auto">
                                        @for (int i = 0; i < liststringMsg().Count(); i++)
                                        {
                                            <a>
                                                <div class="hd-message-sn">
                                                    <div class="hd-message-img" style="line-height: 50px">
                                                        <img style="width:40px" src="~/Areas/Admin/Content/img/logo/Teklogo.png" alt="" />
                                                    </div>
                                                    <div class="hd-mg-ctn">
                                                        <span style="font-size:medium">@liststringMsg()[i]</span>
                                                    </div>
                                                </div>
                                            </a>
                                        }


                                    </div>
                                    <div class="hd-mg-va">
                                        <a>...</a>
                                    </div>
                                </div>
                            </li>
                            <li class="nav-item" title="Dự án TeK">
                                <a data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle"><span><i class="notika-icon notika-menus"></i></span><div class="spinner4 spinner-4"></div><div class="ntd-ctn"><span>@CountProject()</span></div></a>
                                <div role="menu" class="dropdown-menu message-dd task-dd animated zoomIn">
                                    <div class="hd-mg-tt">
                                        <h2>Dự Án Đã và Đang Làm</h2>
                                    </div>
                                    <div class="hd-message-info hd-task-info" style="height:200px;overflow:auto">
                                        <div class="skill">
                                            @for (int i = 0; i < ListProject().Count(); i++)
                                            {
                                                var pt = ListProject()[i].PhantramHoanThanh;
                                                var ptht = ListProject()[i].PhantramHoanThanh + "%";
                                                <div class="progress">
                                                    <div class="lead-content">
                                                        <p>@ListProject()[i].NameProject</p>
                                                    </div>
                                                    <div class="progress-bar wow fadeInLeft" data-progress="@pt" style="width: @ptht" data-wow-duration="1.5s" data-wow-delay="1.2s">
                                                        <span>@pt</span>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="hd-mg-va">
                                        <a href="/Admin/ProjectTeK/Index">View All</a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
                    <div class="col-lg-4 col-md-4 col-sm-5 col-xs-12 col-12">

                        <div class="header-top-menu Topmn">
                            <ul class="nav navbar-nav notika-top-nav">
                                <li>
                                    @if (Session["quyen"] != null)
                                    {
                                        if (Session["quyen"].ToString() == "1")
                                        {
                                            <a href="/Admin/Home/ListUserTeK" class="title_user">UserManager</a>
                                        }
                                        else
                                        {
                                            <a href="/Admin/Home/EditUser/@int.Parse(Session["UserId"].ToString())" class="title_user">Update User</a>
                                        }
                                    }
                                </li>
                                <li><a href="/Admin/Home/LogOut" class="title_user">LogOut</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
