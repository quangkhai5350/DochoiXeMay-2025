                                        <a onclick="mobile('ul-demolibra')" id="ul-demolibra" data-toggle="collapse" data-target="#demolibra" class="colordark" href="#"><i class="notika-icon notika-form"></i> (1)Xuất Nhập @Session["UserName"].ToString().ToUpper()</a>
                                        <ul id="demolibra" class="collapse dropdown-header-top titlemobile">
                                            <li>
                                                @if (Session["quyen"] != null)
                                                {
                                                    <a id="li1-demolibra" onclick="mobile('li1-demolibra')" href="/Admin/XuatNhap/ListXuatNhapUser" class="colordarkChil"> **Bảng Xuất Nhập @Session["UserName"].ToString().ToUpper()**</a>
                                                }
                                            </li>
                                            <li>
                                                <a id="li2-demolibra" onclick="mobile('li2-demolibra')" href="/Admin/DanhMuc/ListHangSanXuat" class="colordarkChil"> Hãng Sản Xuất</a>
