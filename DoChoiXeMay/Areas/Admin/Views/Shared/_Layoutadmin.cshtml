@functions{
    DoChoiXeMay.Models.Model1 db = new DoChoiXeMay.Models.Model1();
    public int CountProject()
    {
        int kqp = db.ProjectTeKs.OrderByDescending(kh => kh.Id)
                    .Skip(0)
                    .Take(7)
                    .Count();
        return kqp;
    }
    public List<DoChoiXeMay.Models.ProjectTeK> ListProject()
    {
        return db.ProjectTeKs
            .OrderByDescending(kh => kh.Id)
            .Skip(0)
            .Take(8)
            .ToList();
    }
    public int CountMsg()
    {
        if(Session["UserId"] != null)
        {
            var uid = int.Parse(Session["UserId"].ToString());
            if (uid == 1)
            {
                return db.MsgAotus.Where(kh => kh.AdminDaxem == false).Count();
            }
            if (uid == 2)
            {
                return db.MsgAotus.Where(kh => kh.Sub2Daxem == false).Count();
            }
            if (uid == 4)
            {
                return db.MsgAotus.Where(kh => kh.Sub4Daxem == false).Count();
            }
            if (uid == 5)
            {
                return db.MsgAotus.Where(kh => kh.Sub5Daxem == false).Count();
            }
            return 0;
        }
        return 0;
    }
    public List<DoChoiXeMay.Models.MsgAotu> ListMsg()
    {
        return db.MsgAotus
            .OrderByDescending(kh => kh.NgayTao)
            .Skip(0)
            .Take(30)
            .ToList();
    }
    public List<string> liststringMsg()
    {
        if (Session["UserId"] != null)
        {
            var uid = int.Parse(Session["UserId"].ToString());
            var lists = ListMsg();
            string[] ListOr = new string[lists.Count()];
            if (lists.Count() > 0)
            {
                for (int i = 0; i < lists.Count(); i++)
                {
                    if (uid == 1 && lists[i].AdminDaxem == false)
                    {
                        ListOr[i] = lists[i].MsgHeThong + "***";
                    }
                    else if (uid == 2 && lists[i].Sub2Daxem == false)
                    {
                        ListOr[i] = lists[i].MsgHeThong + "***";
                    }
                    else if (uid == 4 && lists[i].Sub4Daxem == false)
                    {
                        ListOr[i] = lists[i].MsgHeThong + "***";
                    }
                    else if (uid == 5 && lists[i].Sub5Daxem == false)
                    {
                        ListOr[i] = lists[i].MsgHeThong + "***";
                    }
                    else
                    {
                        ListOr[i] = lists[i].MsgHeThong;
                    }

                }
                return ListOr.ToList();
            }
            return ListOr.ToList();
        }
        else
        {
            var lists = ListMsg();
            string[] ListOr = new string[lists.Count()];
            return ListOr.ToList();
        }
