    <style>
        .logo-Tek {
            padding: 14px 14px 0 0;
            color: white;
            font-weight: bold;
        }

        .color-tile-tek {
            color: mediumblue !important;
        }

        .Topmn {
            margin-right: 10%;
        }

        .titlemobile {
            margin-left: 25%;
        }

        .avatartek {
            width: 34px;
            height: 34px;
            border-radius: 50% !important;
        }
        .colordark{
            color:darkblue;
            font-size:25px;
        }
        .colordarkChil {
            color: darkblue;
            font-size: 20px;
        }
        li:active > a{
           text-decoration:underline
        }
        .Title_Notin {
            font-size: 40px;
            font-weight:bold;
            margin-bottom:20px;
        }
        @@media screen and (max-width:767px) {
            .Topmn {
                /*margin-right: -11%;*/
            }
            .Title_Notin{
                font-size:20px;
                text-align:right;
            }
        }

        @@media (min-width: 768px) and (max-width: 991px) {
            .header-top-menu {
                max-width: 400px;
                margin: 0 auto;
            }
        }
    </style>
